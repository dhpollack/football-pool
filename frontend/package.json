{
  "name": "frontend",
  "private": true,
  "version": "0.4.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:msw": "vite --mode msw",
    "generate": "orval --config orval.config.cjs",
    "build": "tsc --noEmit --project tsconfig.app.json && vite build",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "biome-migrate": "biome migrate",
    "preview": "vite preview",
    "tailwind-init": "tailwindcss init -p",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright test --reporter=html",
    "playwright:install": "playwright install",
    "playwright:install:chromium": "playwright install chromium chromium-headless-shell",
    "playwright:install:firefox": "playwright install firefox",
    "playwright:install:webkit": "playwright install webkit",
    "playwright:show-report": "playwright show-report",
    "msw:init": "msw init public/"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "react": "^19.2.1",
    "react-auth-kit": "^4.0.2-alpha.11",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.4",
    "orval": "^7.17.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
